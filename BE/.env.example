# Development Environment Variables
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nestjs-boilerplate_db
JWT_SECRET=dev_jwt_secret_change_in_production
JWT_EXPIRES_IN=3600s
JWT_REFRESH_SECRET=dev_refresh_jwt_secret_change_in_production
JWT_REFRESH_EXPIRES_IN=7d

# OAuth Configuration (add your own credentials)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret

# App Configuration
FRONTEND_URL=http://localhost:8888
CORS_ORIGINS=http://localhost:8888,http://127.0.0.1:8888
PORT=8888
NODE_ENV=development

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=10
